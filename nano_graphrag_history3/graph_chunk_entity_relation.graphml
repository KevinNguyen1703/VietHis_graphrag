  <data key="d4">8.0</data>
  <data key="d5">"Nguyễn Quốc Trị được chọn là một trong bảy anh hùng tại Đại hội diễn ra vào năm 1952."</data>
  <data key="d6">chunk-6ca8e9b053974b0069f9632265d054f1</data>
  <data key="d7">1</data>
</edge>
<edge source="&quot;ĐẠI HỘI CHIẾN SĨ THI ĐUA VÀ CÁN BỘ GƯƠNG MẪU TOÀN QUỐC LẦN I (1952)&quot;" target="&quot;NGUYỄN THỊ CHIÊN&quot;">
  <data key="d4">8.0</data>
  <data key="d5">"Nguyễn Thị Chiên được chọn là một trong bảy anh hùng tại Đại hội diễn ra vào năm 1952."</data>
  <data key="d6">chunk-6ca8e9b053974b0069f9632265d054f1</data>
  <data key="d7">1</data>
</edge>
