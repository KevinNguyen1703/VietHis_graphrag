  <data key="d4">20.0</data>
  <data key="d5">"Hội nghị quốc tế về Việt Nam được tổ chức để ghi nhận và đảm bảo việc thi hành Hiệp định Paris."</data>
  <data key="d6">chunk-cabe469d0e64ecd74f9a0d0ea1049fc7</data>
  <data key="d7">1</data>
</edge>
