  <data key="d4">16.0</data>
  <data key="d5">"Việt Minh đã chọn Cao Bằng làm một trong những nơi xây dựng căn cứ cách mạng, phát động các hoạt động quân sự và chính trị."</data>
  <data key="d6">chunk-4dd0c7ddfde3a3f5159f7a9aa5a8130e</data>
