  <data key="d4">14.0</data>
  <data key="d5">"Việt Minh lãnh đạo nhân dân Hiệp Hòa (Bắc Giang) nổi dậy chống Nhật."</data>
  <data key="d6">chunk-322292299ce61e02a7c0d12fb0c0dcc1</data>
  <data key="d7">1</data>
</edge>
