  <data key="d4">16.0</data>
  <data key="d5">"Chính quyền Sài Gòn đã phá hoại Hiệp định Paris bằng cách tiến hành các hành động quân sự vi phạm thỏa thuận."</data>
  <data key="d6">chunk-f88841b63107150b5dc7da75070b25a1</data>
  <data key="d7">1</data>
</edge>
