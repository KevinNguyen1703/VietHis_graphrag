  <data key="d4">16.0</data>
  <data key="d5">"Miền Nam Việt Nam phấn đấu thống nhất đất nước qua cách mạng dân tộc dân chủ nhân dân."</data>
  <data key="d6">chunk-5f8692ef5e25f99fa01cb03dd4718eb2</data>
  <data key="d7">1</data>
</edge>
