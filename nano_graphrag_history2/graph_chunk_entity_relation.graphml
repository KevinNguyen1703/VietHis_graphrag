  <data key="d4">16.0</data>
  <data key="d5">"Liên Xô cung cấp sự ủng hộ và giúp đỡ cho Việt Nam trong cuộc kháng chiến."</data>
  <data key="d6">chunk-3dc13d73d4e284ba3bd522c4b5398825</data>
  <data key="d7">1</data>
</edge>
